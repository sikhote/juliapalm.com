{
  "name": "sinclair.tech",
  "description": "Personal website of Julia Palm",
  "author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
  "homepage": "https://juliapalm.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "NODE_ENV=production next start"
  },
  "xo": {
    "prettier": true,
    "extends": "xo-react",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "rules": {
      "react/no-danger": 0,
      "unicorn/filename-case": 0
    },
    "settings": {
      "import/core-modules": [
        "styled-jsx",
        "styled-jsx/css"
      ],
      "react": {
        "version": "16.7.0-alpha.2"
      }
    },
    "env": [
      "browser",
      "node"
    ],
    "space": 2
  },
  "dependencies": {
    "moment": "^2.24.0",
    "next": "7.0.2",
    "polished": "^2.3.3",
    "prop-types": "15.6.2",
    "raw-loader": "1.0.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "showdown": "1.9.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.12.1",
    "eslint-config-xo": "0.26.0",
    "eslint-config-xo-react": "0.18.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "prettier": "1.16.1",
    "xo": "0.24.0"
  }
}
